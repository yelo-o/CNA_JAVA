<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.my.customer.mapper.CustommerMapper">
  <select id="selectById" resultType="com.my.customer.dto.Customer" parameterType="string">
    SELECT * FROM customer WHERE id=#{aaa}
  </select>
  <insert id="insert" parameterType="com.my.customer.dto.Customer">
  	INSERT INTO customer(id,pwd,name) VALUES(#{id},#{pwd},#{name})
  </insert>
</mapper>